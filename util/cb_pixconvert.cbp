				<Option output="pixconvert" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`sdl-config --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`sdl-config --libs`" />
				</Linker>
			</Target>
			<Target title="Windows Debug">
				<Option platforms="Windows;" />
				<Option output="pixconvert" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="-c text.pix#.png" />
				<Compiler>
					<Add option="-g" />
					<Add directory="../../../bdcode/externals/SDL2/include/SDL2" />
					<Add directory="../../../bdcode/externals/SDL_image/include" />
					<Add directory="../../../bdcode/externals/png/include" />
					<Add directory="../../../bdcode/externals/png/include/windows" />
				</Compiler>
				<Linker>
					<Add library="mingw32" />
					<Add library="SDL2main" />
					<Add library="SDL2_image" />
					<Add library="SDL2" />
					<Add library="png" />
