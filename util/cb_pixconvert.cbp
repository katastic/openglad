					<Add library="mingw32" />
					<Add library="SDLmain" />
					<Add library="SDL_image" />
					<Add library="SDL" />
					<Add library="png" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="pixconvert.cpp" />
		<Unit filename="savepng.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="savepng.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
